{
  "expo": {
    "name": "SmartClothingApp",
    "slug": "SmartClothingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-running.png",
    "userInterfaceStyle": "light",
    "scheme": "com.arcs.smartclothingapp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arcs.smartclothingapp"
    },
    "android": {
      "package": "com.arcs.smartclothingapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-running.png",
        "backgroundColor": "#2382e8"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
