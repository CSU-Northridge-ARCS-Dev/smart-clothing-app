{
  "expo": {
    "name": "SmartClothing",
    "slug": "SmartClothingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-running.png",
    "userInterfaceStyle": "light",
    "scheme": "com.arcs.smartclothingapp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arcs.smartclothingapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.arcs.smartclothingapp",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "90fe7b8b-b25e-4389-8373-0f4ba76b7b02"
      }
    },
    "plugins": [
      "expo-font",
      "./plugins/customNamespacePlugin",
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification_icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ]
    ],
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/90fe7b8b-b25e-4389-8373-0f4ba76b7b02"
    }
  }
}
